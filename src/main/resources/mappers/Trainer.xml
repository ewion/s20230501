<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.S20230501.TrainerMapper">
    <select id="getTrainersMemberList" parameterType="String" resultType="String">
        select u.users_name
        from member m, ht_users_data u
        where m.users_id = u.users_id and
              m.trainer_id = #{trainerId}
    </select>
</mapper>